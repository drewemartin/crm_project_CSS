<h1>CRM</h1>

<p>Contacts</p>

<div class="contact_holder">
<% @contacts.each do |contact| %>
<div class="contact_id"><p><%= contact.id %></p></div> 
<div class="contact_first_name"><p><%= contact.first_name %></p></div>
<div class="contact_last_name"><p><%= contact.last_name %></p></div>
<div class="contact_email_address"><p><%= contact.email_address %></p></div>
<div class="contact_notes"><p><%= contact.notes %></p></div>
<% end %>

</div>
